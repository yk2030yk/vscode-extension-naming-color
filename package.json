{
  "name": "vscode-extension-naming-color",
  "displayName": "naming-color",
  "publisher": "takahironegishi",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "naming-color"
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "exportColorJson": "tsc -p ./ && node ./out/color/exportJson.js",
    "jest": "jest"
  },
  "dependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "camelcase": "^6.0.0",
    "color-name-list": "^5.27.0",
    "color-namer": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nearest-color": "^0.4.4",
    "prettier": "^2.0.5",
    "rgb-hex": "^3.0.0",
    "rgb2hex": "^0.2.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/yk2030yk/vscode-extension-naming-color"
  }
}
